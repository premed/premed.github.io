        .col-md-5
          img.featurette-image.img-responsive.center-block(src='http://lorempixel.com/500/500/', alt='Generic placeholder image')
      hr.featurette-divider
      // /END THE FEATURETTES
      // FOOTER
      footer
        p.pull-right
          a(href='#myCarousel') Back to top
        p
          | © 2015 Company, Inc. · 
          a(href='#') Privacy
          |  · 
          a(href='#') Terms
